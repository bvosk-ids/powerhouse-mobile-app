    <icon src="res/icon/ios/Icon-57x57@2x.png" width="114" height="114" />
    <!-- iPad -->
    <icon src="res/icon/ios/Icon-72x72@1x.png" width="72" height="72" />
    <icon src="res/icon/ios/Icon-72x72@2x.png" width="144" height="144" />
